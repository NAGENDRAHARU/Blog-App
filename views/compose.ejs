<%- include('partials/header'); -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="">Title</label><br>
    <input type="text" class="form-control" name="postTitle" autocomplete="off">
    <br>
    <label for="">Post</label><br>
    <textarea class="form-control" name="postBody" rows="8" cols="80"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>
<%- include('partials/footer'); -%>
